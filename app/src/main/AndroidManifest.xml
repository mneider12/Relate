<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nydev.relate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RelationshipDashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RelationshipDetailActivity"
            android:parentActivityName=".RelationshipDashboardActivity">
        </activity>
        <activity android:name=".RelationshipDetailViewActivity"
            android:parentActivityName=".RelationshipDashboardActivity">
        </activity>

    </application>

</manifest>